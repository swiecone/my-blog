 

<%= render 'shared/page_header', bg_image: "img/login-bg.jpg",title: "Log In", subtitle: "Access the back of the site" %>

<div class="row">
	<div class="well col-md-6 col-md-offset-3">
		<%= form_tag '/login' do %>

			<%= label_tag :email %>
			<%= text_field_tag :email %>
			<br>
			<%= label_tag :password %>
			<%= password_field_tag :password %>
			<br>
			<br>

			<%= submit_tag 'Login', class: "btn btn-success" %>
		<% end %>
	</div>
</div>
